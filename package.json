{
  "name": "@piste-icons/react",
  "description": "React components for ski trail difficulty symbols",
  "version": "0.0.0-development",
  "author": "Kirk Eaton <contact@kirkeaton.ca>",
  "bugs": {
    "url": "https://github.com/kirkeaton/piste-icons-react/issues"
  },
  "devDependencies": {
    "@piste-icons/svg": "1.2.0",
    "@svgr/cli": "8.1.0",
    "@types/react": "18.2.46",
    "husky": "9.0.7",
    "lint-staged": "15.2.0",
    "prettier": "3.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "semantic-release": "22.0.12",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=18"
  },
  "exports": {
    "types": "./dist/index.d.ts",
    "default": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/kirkeaton/piste-icons-react#readme",
  "keywords": [
    "piste",
    "ski",
    "trail",
    "difficulty",
    "icons",
    "react"
  ],
  "license": "BSD-3-Clause",
  "lint-staged": {
    "*.{json,md,ts,tsx}": "prettier --write"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kirkeaton/piste-icons-react.git"
  },
  "scripts": {
    "build": "yarn clean && yarn generate && tsc",
    "clean": "find src -type f ! -name 'constants.ts' -exec rm {} \\; && rm -rf dist",
    "generate": "yarn svgr --config-file .svgrrc.cjs node_modules/@piste-icons/svg",
    "prepare": "husky install",
    "prepublishOnly": "yarn build"
  },
  "type": "module"
}
